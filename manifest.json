{
  "name": "QR-Code",
  "manifest_version": 2,
  "description": "Click in Chrome to create a two-dimensional code plugin",
  "version": "1.1.0",
  "icons": {
    "16": "ico/16.png",
    "48": "ico/48.png",
    "128": "ico/128.png"
  },
  "browser_action":{
    "default_title":"Generate QR-Code",
    "default_popup":"index.html"
  },
  "permissions":[
    "tabs",
    "activeTab"
  ]
}
